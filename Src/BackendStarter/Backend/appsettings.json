{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Tokens": {
    "ValidIssuer": "Blazor / Xamarin Hyper Full Stack HOL",
    "ValidAudience": "Blazor / Xamarin Hyper Full Stack HOL",
    "JwtExpireMinutes": 20,
    "JwtRefreshExpireDays": 7,
    "IssuerSigningKey": "https://randomkeygen.com/"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=BackendDB"
  },
  "CustomNLog": {
    "LogRootPath": "C:\\temp"
  }
}
