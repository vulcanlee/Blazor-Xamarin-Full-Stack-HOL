{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Tokens": {
    "ValidIssuer": "Backend Hyper Full Stack HOL",
    "ValidAudience": "Backend Hyper Full Stack HOL",
    "JwtExpireMinutes": 20,
    "JwtRefreshExpireDays": 7,
    "IssuerSigningKey": "https://randomkeygen.com/"
  },
  "ConnectionStrings": {
    "BackendDefaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=BackendDB;MultipleActiveResultSets=True"
  },
  "CustomNLog": {
    "LogRootPath": "C:\\temp",
    "AllLogMessagesFilename": "BackendAllLog",
    "AllWebDetailsLogMessagesFilename": "BackendAllWeb"
  },
  "KeepAliveEndpoint": "https://backendhol.azurewebsites.net/KeepAlive",
  "EnableKeepAliveEndpoint": true,
  "ShowImportanceConfigurationInforation": true,
  "EmergenceDebug": true,
  "AdministratorPassword": "123",
  "CheckUserStateInterval": 120000
}
